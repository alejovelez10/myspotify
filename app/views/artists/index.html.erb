<div class="container">
	

<div class="row index">
<% @artists.each do |artist|%>

  <div class="col-sm-3 col-md-4">
    <div class="thumbnail">
      <%= image_tag (artist.image_url)%>
      <div class="caption">
        <h3><%= artist.name%></h3>
        <p><%= link_to "ver", artist, class: "btn btn-primary btn-block"%></p>
      </div>
    </div>
  </div>

<%end%>
</div>
</div>